{
  "index_patterns": [
    "__SEARCH_INDEX_PREFIX__stats-file-download-*"
  ],
  "settings": {
    "index": {
      "refresh_interval": "5s"
    }
  },
  "mappings": {
    "dynamic_templates": [
      {
        "date_fields": {
          "match_mapping_type": "date",
          "mapping": {
            "type": "date",
            "format": "date_optional_time"
          }
        }
      }
    ],
    "date_detection": false,
    "dynamic": "strict",
    "numeric_detection": false,
    "properties": {
      "timestamp": {
        "type": "date",
        "format": "date_optional_time"
      },
      "count": {
        "type": "integer"
      },
      "unique_count": {
        "type": "integer"
      },
      "file": {
        "type": "keyword"
      },
      "unique_id": {
        "type": "keyword"
      },
      "updated_timestamp": {
        "type": "date"
      }
    }
  },
  "aliases": {
    "__SEARCH_INDEX_PREFIX__stats-file-download": {}
  }
}